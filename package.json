{
  "name": "findx",
  "displayName": "FindX",
  "description": "Extensión para buscar una palabra en todos los archivos del proyecto en VS Code de manera rápida y eficiente",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.searchWord",
        "title": "Buscar palabra en todos los archivos"
      }
    ],
    "keybindings": [
      {
        "command": "extension.searchWord",
        "key": "ctrl+alt+f",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
